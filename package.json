  "dependencies": {
    "axios": "^0.18.0",
    "body-parser": "^1.18.3",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "gsap": "^2.0.2",
    "node-sass-chokidar": "^1.3.4",
    "nodemailer": "^5.1.1",
    "npm-run-all": "^4.1.5",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-image-gallery": "^0.8.12",
    "react-modal": "^3.8.1",
    "react-redux": "^6.0.0",
